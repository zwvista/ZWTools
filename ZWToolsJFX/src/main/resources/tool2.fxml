<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zwstudio.tools.controllers.Tool2Controller">
   <center>
      <TableView fx:id="tblMessages" prefHeight="461.0" prefWidth="593.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="tcSheetName" prefWidth="88.0" text="SheetName" />
          <TableColumn fx:id="tcId" prefWidth="88.0" text="ID" />
          <TableColumn fx:id="tcMessage" prefWidth="160.0" text="Message" />
        </columns>
      </TableView>
   </center>
   <top>
      <HBox>
         <children>
            <Label text="Filter:" />
            <TextField fx:id="tfFilter" />
            <Button id="btnLoad" mnemonicParsing="false" onAction="#btnLoadOnAction" text="Load" BorderPane.alignment="CENTER" />
         </children>
      </HBox>
   </top>
</BorderPane>
